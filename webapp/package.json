{
  "name": "nextjs-template",
  "version": "1.0.0",
  "description": "A template for Nextjs projects.",
  "author": "Milkyway Technology",
  "repository": "https://github.com/milkyway-technology/nextjs-template",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "start:dev": "next dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "start-test start:dev http://localhost:3000 'cypress run --config-file ./test/cypress.config.ts'",
    "test:e2e:watch": "start-test start:dev http://localhost:3000 'cypress open --config-file ./test/cypress.config.ts'",
    "test:e2e:cov": "start-test start:dev http://localhost:3000 'cypress run --config-file ./test/cypress.config.ts && nyc report --reporter=text-summary --report-dir=test/coverage --temp-dir=test/.nyc_output'"
  },
  "dependencies": {
    "@circle-fin/w3s-pw-web-sdk": "^1.0.9",
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@reduxjs/toolkit": "^1.9.7",
    "autoprefixer": "^10.4.14",
    "crypto-js": "^4.2.0",
    "next": "^13.4.8",
    "postcss": "^8.4.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-toastify": "^10.0.5",
    "reactflow": "^11.10.4",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@babel/preset-env": "^7.22.7",
    "@cypress/code-coverage": "^3.10.8",
    "@next/eslint-plugin-next": "^13.4.8",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/crypto-js": "^4.2.1",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^12.16.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.4.8",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "jest": "^29.6.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1"
  }
}
